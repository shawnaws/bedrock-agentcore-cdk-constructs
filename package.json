{
  "name": "bedrock-agentcore-cdk-constructs",
  "version": "0.1.2",
  "description": "AWS CDK constructs for Bedrock Agent Core and supporting components",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shawnaws/bedrock-agentcore-cdk-constructs.git"
  },
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf lib",
    "prepare": "npm run build",
    "docs": "typedoc src/index.ts"
  },
  "keywords": [
    "aws",
    "cdk",
    "constructs",
    "bedrock",
    "agent-core",
    "knowledge-base",
    "infrastructure"
  ],
  "author": "Shawn Chasse",
  "license": "MIT",
  "files": [
    "lib/**/*",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@cdklabs/generative-ai-cdk-constructs": "^0.1.0",
    "aws-cdk-lib": "^2.100.0",
    "bedrock-agentcore-cdk-constructs": "github:shawnaws/bedrock-agentcore-cdk-constructs",
    "constructs": "^10.0.0",
    "fs": "^0.0.1-security"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.100.0",
    "constructs": "^10.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "directories": {
    "doc": "docs",
    "example": "examples",
    "lib": "lib",
    "test": "test"
  },
  "bugs": {
    "url": "https://github.com/shawnaws/bedrock-agentcore-cdk-constructs/issues"
  },
  "homepage": "https://github.com/shawnaws/bedrock-agentcore-cdk-constructs#readme"
}
